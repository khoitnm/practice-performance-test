{
  "request": {
    "method": "GET",
    "urlPath": "/api/entity",
    "queryParameters": {
      "name": {
        "matches": ".*"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": "{{randomValue length=10 type='NUMERIC'}}",
      "name": "{{request.query.name}}"
    }
  }
}